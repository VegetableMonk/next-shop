{
	"$schema": "https://biomejs.dev/schemas/1.3.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious":{
				"noExplicitAny": false
			},
			"style":{
				"noNegationElse":true,
				"useNamingConvention":true,
			},
			"complexity":{
				"useSimplifiedLogicExpression":true,
				"noExcessiveCognitiveComplexity":true
			},
			"correctnes":true
			
		}
	},
	"formatter":{
		"enabled":true,
		"semicolons": "asNeeded",
		
	}
}
